	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-BC,B' rate='0.04386898951229944'>
		BC:1 -> B:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,BC' rate='0.11831189586264212'>
