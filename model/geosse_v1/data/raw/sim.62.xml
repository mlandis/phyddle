<populationType spec='PopulationType' typeName='AC' id='AC'/>
<populationType spec='PopulationType' typeName='BC' id='BC'/>
<populationType spec='PopulationType' typeName='ABC' id='ABC'/>

<reactionGroup spec='ReactionGroup' reactionGroupName='Extinction'>
	<reaction spec='Reaction' reactionName='Extinction-A' rate='0.08868476515175841'>
		A:1 -> X
	</reaction>
	<reaction spec='Reaction' reactionName='Extinction-B' rate='0.17368975666671865'>
		B:1 -> X
	</reaction>
	<reaction spec='Reaction' reactionName='Extinction-C' rate='0.12021720093738644'>
		C:1 -> X
	</reaction>
</reactionGroup>

<reactionGroup spec='ReactionGroup' reactionGroupName='Extirpation'>
	<reaction spec='Reaction' reactionName='Extirpation-AB,B' rate='0.08868476515175841'>
		AB:1 -> B:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-AB,A' rate='0.17368975666671865'>
		AB:1 -> A:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-AC,C' rate='0.08868476515175841'>
		AC:1 -> C:1
	</reaction>
