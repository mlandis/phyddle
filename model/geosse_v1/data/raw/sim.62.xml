<reactionGroup spec='ReactionGroup' reactionGroupName='Dispersal'>
	<reaction spec='Reaction' reactionName='Dispersal-A,AB' rate='0.043933914306084995'>
		A:1 -> AB:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-A,AC' rate='0.13935841902773247'>
