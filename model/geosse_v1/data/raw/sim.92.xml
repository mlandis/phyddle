	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-BC,BC,C' rate='0.005652084308457056'>
		BC:1 -> BC:1 + C:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-ABC,ABC,A' rate='0.014735692367002779'>
		ABC:1 -> ABC:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-ABC,ABC,B' rate='0.007380714176844838'>
