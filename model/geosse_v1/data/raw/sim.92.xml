<reactionGroup spec='ReactionGroup' reactionGroupName='Dispersal'>
	<reaction spec='Reaction' reactionName='Dispersal-A,AB' rate='0.036321601532278855'>
		A:1 -> AB:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-A,AC' rate='0.08059597319658122'>
