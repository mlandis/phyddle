	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-C,AC' rate='0.23128110167737947'>
		C:1 -> AC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-C,BC' rate='0.09319833400420634'>
		C:1 -> BC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AB,ABC' rate='0.115702644903017'>
		AB:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AC,ABC' rate='0.1295199355364852'>
		AC:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-BC,ABC' rate='0.3906796540819607'>
		BC:1 -> ABC:1
	</reaction>
