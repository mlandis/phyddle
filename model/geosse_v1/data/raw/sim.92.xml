<populationType spec='PopulationType' typeName='BC' id='BC'/>
<populationType spec='PopulationType' typeName='ABC' id='ABC'/>

<reactionGroup spec='ReactionGroup' reactionGroupName='Extinction'>
	<reaction spec='Reaction' reactionName='Extinction-A' rate='0.10566420876330039'>
		A:1 -> X
	</reaction>
	<reaction spec='Reaction' reactionName='Extinction-B' rate='0.016169930367624163'>
		B:1 -> X
	</reaction>
	<reaction spec='Reaction' reactionName='Extinction-C' rate='0.02047102175223342'>
		C:1 -> X
	</reaction>
</reactionGroup>

<reactionGroup spec='ReactionGroup' reactionGroupName='Extirpation'>
	<reaction spec='Reaction' reactionName='Extirpation-AB,B' rate='0.10566420876330039'>
		AB:1 -> B:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-AB,A' rate='0.016169930367624163'>
