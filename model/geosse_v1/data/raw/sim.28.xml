	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-BC,BC,C' rate='0.006604980245530432'>
		BC:1 -> BC:1 + C:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-ABC,ABC,A' rate='0.013865863883613834'>
		ABC:1 -> ABC:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-ABC,ABC,B' rate='0.03838742306574899'>
