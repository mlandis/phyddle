<populationType spec='PopulationType' typeName='ABC' id='ABC'/>

<reactionGroup spec='ReactionGroup' reactionGroupName='Extinction'>
	<reaction spec='Reaction' reactionName='Extinction-A' rate='0.0532021489247303'>
		A:1 -> X
	</reaction>
	<reaction spec='Reaction' reactionName='Extinction-B' rate='0.032524305421816654'>
		B:1 -> X
	</reaction>
	<reaction spec='Reaction' reactionName='Extinction-C' rate='0.0023256108578751543'>
