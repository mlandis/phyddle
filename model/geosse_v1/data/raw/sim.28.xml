	<reaction spec='Reaction' reactionName='Extirpation-AB,B' rate='0.0532021489247303'>
		AB:1 -> B:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-AB,A' rate='0.032524305421816654'>
		AB:1 -> A:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-AC,C' rate='0.0532021489247303'>
