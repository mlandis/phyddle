<reactionGroup spec='ReactionGroup' reactionGroupName='Dispersal'>
	<reaction spec='Reaction' reactionName='Dispersal-A,AB' rate='0.3681368419845019'>
		A:1 -> AB:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-A,AC' rate='0.021930264008339008'>
