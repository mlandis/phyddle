	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-BC,C' rate='0.032524305421816654'>
		BC:1 -> C:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-BC,B' rate='0.0023256108578751543'>
		BC:1 -> B:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,BC' rate='0.0532021489247303'>
		ABC:1 -> BC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,AC' rate='0.032524305421816654'>
		ABC:1 -> AC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,AB' rate='0.0023256108578751543'>
		ABC:1 -> AB:1
	</reaction>
</reactionGroup>

<reactionGroup spec='ReactionGroup' reactionGroupName='WithinRegionSpeciation'>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-A,A,A' rate='0.013865863883613834'>
		A:1 -> A:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-B,B,B' rate='0.03838742306574899'>
		B:1 -> B:1 + B:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-C,C,C' rate='0.006604980245530432'>
		C:1 -> C:1 + C:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AB,AB,A' rate='0.013865863883613834'>
		AB:1 -> AB:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AB,AB,B' rate='0.03838742306574899'>
		AB:1 -> AB:1 + B:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AC,AC,A' rate='0.013865863883613834'>
		AC:1 -> AC:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AC,AC,C' rate='0.006604980245530432'>
		AC:1 -> AC:1 + C:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-BC,BC,B' rate='0.03838742306574899'>
		BC:1 -> BC:1 + B:1
