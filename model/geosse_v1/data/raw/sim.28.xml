	<reaction spec='Reaction' reactionName='Dispersal-C,AC' rate='0.008035992595212274'>
		C:1 -> AC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-C,BC' rate='0.04569821733675947'>
		C:1 -> BC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AB,ABC' rate='0.35472774570084503'>
		AB:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AC,ABC' rate='0.4138350593212614'>
		AC:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-BC,ABC' rate='0.0398148098562346'>
		BC:1 -> ABC:1
	</reaction>
