<populationType spec='PopulationType' typeName='BC' id='BC'/>
<populationType spec='PopulationType' typeName='ABC' id='ABC'/>

<reactionGroup spec='ReactionGroup' reactionGroupName='Extinction'>
	<reaction spec='Reaction' reactionName='Extinction-A' rate='0.2545148271936396'>
		A:1 -> X
	</reaction>
	<reaction spec='Reaction' reactionName='Extinction-B' rate='0.20151440807810783'>
		B:1 -> X
	</reaction>
	<reaction spec='Reaction' reactionName='Extinction-C' rate='0.40130788387752575'>
		C:1 -> X
	</reaction>
</reactionGroup>

<reactionGroup spec='ReactionGroup' reactionGroupName='Extirpation'>
	<reaction spec='Reaction' reactionName='Extirpation-AB,B' rate='0.2545148271936396'>
		AB:1 -> B:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-AB,A' rate='0.20151440807810783'>
