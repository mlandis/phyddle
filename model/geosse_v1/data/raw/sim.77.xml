<reactionGroup spec='ReactionGroup' reactionGroupName='WithinRegionSpeciation'>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-A,A,A' rate='0.03281747773755393'>
		A:1 -> A:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-B,B,B' rate='0.18808575433931085'>
		B:1 -> B:1 + B:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-C,C,C' rate='0.020124897749376043'>
		C:1 -> C:1 + C:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AB,AB,A' rate='0.03281747773755393'>
		AB:1 -> AB:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AB,AB,B' rate='0.18808575433931085'>
		AB:1 -> AB:1 + B:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AC,AC,A' rate='0.03281747773755393'>
		AC:1 -> AC:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AC,AC,C' rate='0.020124897749376043'>
