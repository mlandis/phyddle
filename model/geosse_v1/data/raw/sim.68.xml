	</reaction>
</reactionGroup>

<reactionGroup spec='ReactionGroup' reactionGroupName='WithinRegionSpeciation'>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-A,A,A' rate='0.028504575766285917'>
		A:1 -> A:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-B,B,B' rate='0.016546232537043102'>
		B:1 -> B:1 + B:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-C,C,C' rate='0.37124750837028336'>
		C:1 -> C:1 + C:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AB,AB,A' rate='0.028504575766285917'>
		AB:1 -> AB:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AB,AB,B' rate='0.016546232537043102'>
		AB:1 -> AB:1 + B:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AC,AC,A' rate='0.028504575766285917'>
		AC:1 -> AC:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AC,AC,C' rate='0.37124750837028336'>
