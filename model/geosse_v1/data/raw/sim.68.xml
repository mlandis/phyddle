	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-BC,C' rate='0.0035725204418056503'>
		BC:1 -> C:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-BC,B' rate='0.21679690591175796'>
		BC:1 -> B:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,BC' rate='0.2419511409082043'>
		ABC:1 -> BC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,AC' rate='0.0035725204418056503'>
