	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AB,ABC' rate='0.020126188130737552'>
		AB:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AC,ABC' rate='0.11537283861663503'>
		AC:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-BC,ABC' rate='0.5286748531667299'>
