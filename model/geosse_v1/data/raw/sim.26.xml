	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AB,ABC' rate='0.1769173013193163'>
		AB:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AC,ABC' rate='0.23316436373477378'>
		AC:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-BC,ABC' rate='0.2995203970727639'>
