<reactionGroup spec='ReactionGroup' reactionGroupName='Dispersal'>
	<reaction spec='Reaction' reactionName='Dispersal-A,AB' rate='0.04779030366115696'>
		A:1 -> AB:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-A,AC' rate='0.2381116998219207'>
		A:1 -> AC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-B,AB' rate='0.06475052065359375'>
