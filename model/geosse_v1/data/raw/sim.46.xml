	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AB,ABC' rate='0.09974033055908389'>
		AB:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AC,ABC' rate='0.275631248622326'>
		AC:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-BC,ABC' rate='0.21182701817212873'>
