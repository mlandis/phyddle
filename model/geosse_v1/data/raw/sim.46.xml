<reactionGroup spec='ReactionGroup' reactionGroupName='Extinction'>
	<reaction spec='Reaction' reactionName='Extinction-A' rate='0.006128637688345434'>
		A:1 -> X
	</reaction>
	<reaction spec='Reaction' reactionName='Extinction-B' rate='0.002461351519758523'>
