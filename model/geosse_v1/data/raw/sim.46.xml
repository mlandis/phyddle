	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-BC,C' rate='0.002461351519758523'>
		BC:1 -> C:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-BC,B' rate='0.181664991933267'>
		BC:1 -> B:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,BC' rate='0.006128637688345434'>
		ABC:1 -> BC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,AC' rate='0.002461351519758523'>
