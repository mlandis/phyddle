<populationType spec='PopulationType' typeName='ABC' id='ABC'/>

<reactionGroup spec='ReactionGroup' reactionGroupName='Extinction'>
	<reaction spec='Reaction' reactionName='Extinction-A' rate='0.07280537015828915'>
		A:1 -> X
	</reaction>
	<reaction spec='Reaction' reactionName='Extinction-B' rate='0.13601143752690908'>
		B:1 -> X
	</reaction>
	<reaction spec='Reaction' reactionName='Extinction-C' rate='0.16225228728948252'>
