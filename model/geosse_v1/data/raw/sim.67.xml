	<reaction spec='Reaction' reactionName='Extirpation-AB,B' rate='0.07280537015828915'>
		AB:1 -> B:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-AB,A' rate='0.13601143752690908'>
