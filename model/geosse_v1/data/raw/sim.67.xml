	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-BC,C' rate='0.13601143752690908'>
		BC:1 -> C:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-BC,B' rate='0.16225228728948252'>
		BC:1 -> B:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,BC' rate='0.07280537015828915'>
		ABC:1 -> BC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,AC' rate='0.13601143752690908'>
