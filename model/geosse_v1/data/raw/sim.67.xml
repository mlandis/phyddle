	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AB,ABC' rate='0.21267011244688167'>
		AB:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AC,ABC' rate='0.19578387329805363'>
		AC:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-BC,ABC' rate='0.2566462351042099'>
		BC:1 -> ABC:1
	</reaction>
