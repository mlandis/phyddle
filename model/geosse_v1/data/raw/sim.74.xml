<reactionGroup spec='ReactionGroup' reactionGroupName='Dispersal'>
	<reaction spec='Reaction' reactionName='Dispersal-A,AB' rate='0.07227897396224571'>
		A:1 -> AB:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-A,AC' rate='0.45395514564302436'>
