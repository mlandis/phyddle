	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-BC,C' rate='0.3417033436651127'>
		BC:1 -> C:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-BC,B' rate='0.02753772558897344'>
		BC:1 -> B:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,BC' rate='0.14277727967864604'>
		ABC:1 -> BC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,AC' rate='0.3417033436651127'>
