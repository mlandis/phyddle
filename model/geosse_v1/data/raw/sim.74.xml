	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AB,ABC' rate='0.49989141676619914'>
		AB:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-AC,ABC' rate='0.29927036192438833'>
		AC:1 -> ABC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Dispersal-BC,ABC' rate='0.0829789036281536'>
