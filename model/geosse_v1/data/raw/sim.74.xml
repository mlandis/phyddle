	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,AB' rate='0.02753772558897344'>
		ABC:1 -> AB:1
	</reaction>
</reactionGroup>

<reactionGroup spec='ReactionGroup' reactionGroupName='WithinRegionSpeciation'>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-A,A,A' rate='0.051861527657874856'>
		A:1 -> A:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-B,B,B' rate='0.09545881769356872'>
		B:1 -> B:1 + B:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-C,C,C' rate='0.05024203578709363'>
		C:1 -> C:1 + C:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AB,AB,A' rate='0.051861527657874856'>
		AB:1 -> AB:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AB,AB,B' rate='0.09545881769356872'>
		AB:1 -> AB:1 + B:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AC,AC,A' rate='0.051861527657874856'>
		AC:1 -> AC:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AC,AC,C' rate='0.05024203578709363'>
		AC:1 -> AC:1 + C:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-BC,BC,B' rate='0.09545881769356872'>
