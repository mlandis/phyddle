	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-BC,B' rate='0.05579884309780886'>
		BC:1 -> B:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,BC' rate='0.06917686447028007'>
		ABC:1 -> BC:1
	</reaction>
	<reaction spec='Reaction' reactionName='Extirpation-ABC,AC' rate='0.00398234158221233'>
