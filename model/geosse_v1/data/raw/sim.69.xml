<reactionGroup spec='ReactionGroup' reactionGroupName='WithinRegionSpeciation'>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-A,A,A' rate='0.0806929119359654'>
		A:1 -> A:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-B,B,B' rate='0.00986136694996007'>
		B:1 -> B:1 + B:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-C,C,C' rate='0.025807504252977915'>
		C:1 -> C:1 + C:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AB,AB,A' rate='0.0806929119359654'>
		AB:1 -> AB:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AB,AB,B' rate='0.00986136694996007'>
		AB:1 -> AB:1 + B:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AC,AC,A' rate='0.0806929119359654'>
		AC:1 -> AC:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AC,AC,C' rate='0.025807504252977915'>
