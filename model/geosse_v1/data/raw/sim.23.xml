	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-C,C,C' rate='0.054001347076503704'>
		C:1 -> C:1 + C:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AB,AB,A' rate='0.05834462025792689'>
