<reactionGroup spec='ReactionGroup' reactionGroupName='WithinRegionSpeciation'>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-A,A,A' rate='0.033216339719020264'>
		A:1 -> A:1 + A:1
