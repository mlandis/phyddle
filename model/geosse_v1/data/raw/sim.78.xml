	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-C,C,C' rate='0.052081291046229786'>
		C:1 -> C:1 + C:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AB,AB,A' rate='0.033216339719020264'>
		AB:1 -> AB:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AB,AB,B' rate='0.04120596102961974'>
		AB:1 -> AB:1 + B:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AC,AC,A' rate='0.033216339719020264'>
		AC:1 -> AC:1 + A:1
	</reaction>
	<reaction spec='Reaction' reactionName='WithinRegionSpeciation-AC,AC,C' rate='0.052081291046229786'>
