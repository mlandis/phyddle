	</reaction>
	<reaction spec='Reaction' reactionName='w_6_6_1' rate='0.4999430258138836'>
		S[6]:1 -> S[6]:1 + S[1]:1 + G[1]
	</reaction>
	<reaction spec='Reaction' reactionName='w_6_6_2' rate='0.4999430258138836'>
		S[6]:1 -> S[6]:1 + S[2]:1 + G[2]
	</reaction>
</reactionGroup>



</model>

<initialState spec='InitState'>
	<populationSize spec='PopulationSize' size='1'>
		<population spec='Population' type='@G' location='0'/>
	</populationSize>
	<populationSize spec='PopulationSize' size='1'>
		<population spec='Population' type='@G' location='1'/>
	</populationSize>
	<lineageSeedMultiple spec='MultipleIndividuals' copies='1'>
		<population spec ='Population' type='@S' location='3'/>
	</lineageSeedMultiple>
</initialState>


<lineageEndCondition spec='LineageEndCondition' nLineages='500' alsoGreaterThan='true' isRejection='false'/>


<inheritancePostProcessor spec='LineageFilter' populationName='S' reverseTime='false' discard='false' leavesOnly='true' noClean='true'/>


<output spec='NewickOutput' collapseSingleChildNodes='true' fileName='../raw_data/example/sim.15.tre'/>
<output spec='NexusOutput' fileName='../raw_data/example/sim.15.phy.nex'/>
<output spec='JsonOutput' fileName='../raw_data/example/sim.15.json' />

</run>
</beast>
