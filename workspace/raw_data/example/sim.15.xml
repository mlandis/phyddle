	</reaction>
	<reaction spec='Reaction' reactionName='e_3_0' rate='0.1522330660779037'>
		S[3]:1 -> S[0]:1 + L[1]
	</reaction>
	<reaction spec='Reaction' reactionName='e_4_2' rate='0.1522330660779037'>
		S[4]:1 -> S[2]:1 + L[0]
	</reaction>
	<reaction spec='Reaction' reactionName='e_4_0' rate='0.1522330660779037'>
		S[4]:1 -> S[0]:1 + L[2]
	</reaction>
	<reaction spec='Reaction' reactionName='e_5_2' rate='0.1522330660779037'>
