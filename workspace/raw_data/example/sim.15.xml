	</reaction>
	<reaction spec='Reaction' reactionName='r_x_2' rate='0.1522330660779037'>
		S[2]:1 -> X[2] + L[2]
	</reaction>
</reactionGroup>

<reactionGroup spec='ReactionGroup' reactionGroupName='Dispersal'>
	<reaction spec='Reaction' reactionName='d_0_3' rate='0.029016625867392832'>
