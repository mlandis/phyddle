	</reaction>
</reactionGroup>

<reactionGroup spec='ReactionGroup' reactionGroupName='Extinction'>
	<reaction spec='Reaction' reactionName='r_x_0' rate='0.1522330660779037'>
		S[0]:1 -> X[0] + L[0]
	</reaction>
	<reaction spec='Reaction' reactionName='r_x_1' rate='0.1522330660779037'>
