	</reaction>
	<reaction spec='Reaction' reactionName='d_3_6' rate='0.058033251734785664'>
		S[3]:1 -> S[6]:1 + G[2]
	</reaction>
	<reaction spec='Reaction' reactionName='d_4_6' rate='0.058033251734785664'>
		S[4]:1 -> S[6]:1 + G[1]
	</reaction>
	<reaction spec='Reaction' reactionName='d_5_6' rate='0.058033251734785664'>
